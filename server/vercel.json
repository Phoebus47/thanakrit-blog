{
  "version": 2,
  "builds": [
    {
      "src": "src/app.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "prisma/schema.prisma",
          ".env.production"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.mjs"
    }
  ]
}