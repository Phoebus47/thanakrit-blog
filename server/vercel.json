{
  "version": 2,
  "builds": [
    {
      "src": "app.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "prisma/schema.prisma",
          "node_modules/@prisma/client/**",
          "node_modules/.prisma/client/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.mjs"
    }
  ],
  "buildCommand": "npm install && npm run build",
  "ignoreCommand": "echo ''"
}
