{
  "version": 2,
  "builds": [
    {
      "src": "src/app.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "prisma/schema.prisma",
          ".env.production"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/app.mjs"
    }
  ],
  "env": {
    "JWT_SECRET": "@jwt-secret",
    "DATABASE_URL": "@database-url",
    "DIRECT_URL": "@direct-url"
  }
}