{
  "version": 2,
  "builds": [
    {
      "src": "app.mjs",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/schema.prisma"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.mjs"
    }
  ],
  "buildCommand": "npm install && npx prisma generate && npm run build",
  "outputDirectory": "dist",
  "ignoreCommand": "echo ''"
}
